keywords = {Animals,Anticonvulsants,Anticonvulsants: pharmacology,Anticonvulsants: therapeutic use,Cannabidiol,Cannabidiol: administration {\&} dosage,Cannabidiol: pharmacology,Disease Models, Animal,Male,Rats,Rats, Inbred WKY,Seizures,Seizures: chemically induced,Seizures: drug therapy,Temporal Lobe,Temporal Lobe: drug effects,Treatment Outcome},
month = {jun},
