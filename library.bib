pmid = {20519329},
title = {{Challenges of sequencing human genomes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2980933{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {11},
year = {2010}
}
