isbn = {2229-5089 (Print)},
issn = {2229-5089},
journal = {Journal of pathology informatics},
keywords = {Breast Cancer,Histopathology,ICPR Contest 2012,Mitosis Detection},
pages = {8},
pmid = {23858383},
title = {{Mitosis detection in breast cancer histological images An ICPR 2012 contest.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3709417{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {4},
year = {2013}
}
