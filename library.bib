journal = {Molecular cancer},
keywords = {Adenocarcinoma,Adenocarcinoma: genetics,Alternative Splicing,Blotting, Western,Cell Line, Tumor,Humans,Lung Neoplasms,Lung Neoplasms: genetics,Molecular Targeted Therapy,Nuclear Proteins,Nuclear Proteins: genetics,Oligonucleotide Array Sequence Analysis,Oligonucleotide Array Sequence Analysis: methods,RNA Precursors,RNA Precursors: genetics,Real-Time Polymerase Chain Reaction,Ribonucleoproteins,Ribonucleoproteins: genetics},
month = {jan},
