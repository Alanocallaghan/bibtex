isbn = {1932-6203},
issn = {19326203},
journal = {PLoS ONE},
number = {12},
pmid = {22174928},
title = {{Where do neurologists look when viewing brain CT images? an eye-tracking study involving stroke cases}},
volume = {6},
year = {2011}
}
