@article{Chen2012,
  title = {Drug-Target Interaction Prediction by Random Walk on the Heterogeneous Network},
  volume = {8},
  issn = {1742206X},
  doi = {10.1039/c2mb00002d},
  abstract = {Predicting potential drug\textendash{}target interactions from heterogeneous biological data is critical not only for better understanding of the various interactions and biological processes, but also for the development of novel drugs and the improvement of human medicines. In this paper, the method of Network-based Random Walk with Restart on the Heterogeneous network (NRWRH) is developed to predict potential drug\textendash{}target interactions on a large scale under the hypothesis that similar drugs often target similar target proteins and the framework of Random Walk. Compared with traditional supervised or semi-supervised methods, NRWRH makes full use of the tool of the network for data integration to predict drug\textendash{}target associations. It integrates three different networks (protein\textendash{}protein similarity network, drug\textendash{}drug similarity network, and known drug\textendash{}target interaction networks) into a heterogeneous network by known drug\textendash{}target interactions and implements the random walk on this heterogeneous network. When applied to four classes of important drug\textendash{}target interactions including enzymes, ion channels, GPCRs and nuclear receptors, NRWRH significantly improves previous methods in terms of cross-validation and potential drug\textendash{}target interaction prediction. Excellent performance enables us to suggest a number of new potential drug\textendash{}target interactions for drug development.},
  number = {7},
  journal = {Molecular BioSystems},
  author = {Chen, Xing and Liu, Ming Xi and Yan, Gui Ying},
  year = {2012},
  pages = {1970-1978},
  file = {/home/alan/Zotero/storage/S952N2VR/Chen et al_2012_Drug-target interaction prediction by random walk on the heterogeneous network.pdf},
  pmid = {22538619}
}

