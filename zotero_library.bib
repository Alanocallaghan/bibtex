  journal = {Food and chemical toxicology : an international journal published for the British Industrial Biological Research Association},
  author = {Park, Hye Young and Han, Min Ho and Park, Cheol and Jin, Cheng-Yun and Kim, Gi-Young and Choi, Il-Whan and Kim, Nam Deuk and Nam, Taek-Jeong and Kwon, Taeg Kyu and Choi, Yung Hyun},
