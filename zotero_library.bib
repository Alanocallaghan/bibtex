  journal = {Clinical genitourinary cancer},
  author = {Necchi, Andrea and Nicolai, Nicola and Mariani, Luigi and Lo Vullo, Salvatore and Giannatempo, Patrizia and Raggi, Daniele and Far\`e, Elena and Piva, Luigi and Biasoni, Davide and Catanzaro, Mario and Torelli, Tullio and Stagni, Silvia and Milani, Angelo and Gianni, Alessandro M and Salvioni, Roberto},
