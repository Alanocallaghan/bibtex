  month = jan,
  year = {2010},
  keywords = {Animals,Mice,Alzheimer Disease,Alzheimer Disease: drug therapy,Alzheimer Disease: metabolism,Alzheimer Disease: pathology,Amyloid beta-Peptides,Amyloid beta-Peptides: antagonists \& inhibitors,Amyloid beta-Peptides: metabolism,Cell Aggregation,Cell Aggregation: drug effects,Mice; Transgenic,Phytotherapy,Phytotherapy: methods,Plant Extracts},
  pages = {1359-70},
