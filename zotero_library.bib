  journal = {The Journal of urology},
  author = {{de Martino}, Michela and Haitel, Andrea and Schatzl, Georg and Klatte, Tobias},
  month = oct,
  year = {2013},
  keywords = {Humans,Male,Middle Aged,Female,Genetic Variation,Carcinoma; Renal Cell,Carcinoma; Renal Cell: genetics,Kidney Neoplasms,Kidney Neoplasms: genetics,Kidney Neoplasms: pathology,Carcinoma; Renal Cell: mortality,Kidney Neoplasms: mortality,Retrospective Studies,Survival Rate,Carcinoma; Renal Cell: secondary,Polymorphism; Restriction Fragment Length,Receptor; PAR-1,Receptor; PAR-1: genetics,\#nosource},
