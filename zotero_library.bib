  author = {AHN, SUNG-JA and CHOI, CHAN and CHOI, YOO-DUK and KIM, YOUNG-CHUL and KIM, KYU-SIK and OH, IN-JAE and BAN, HEE-JUNG and YOON, MEE-SUN and NAM, TAEK-KEUN and JEONG, JAE-UK and SONG, JOO-YOUNG and CHUNG, WOONG-KI},
