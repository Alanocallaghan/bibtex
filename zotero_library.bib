  journal = {PLoS genetics},
  author = {Dallosso, Anthony R and Hancock, Anne L and Szemes, Marianna and Moorwood, Kim and Chilukamarri, Laxmi and Tsai, Hsin-Hao and Sarkar, Abby and Barasch, Jonathan and Vuononvirta, Raisa and Jones, Chris and {Pritchard-Jones}, Kathy and {Royer-Pokora}, Brigitte and Lee, Sean Bong and Owen, Ceris and Malik, Sally and Feng, Yi and Frank, Marcus and Ward, Andrew and Brown, Keith W and Malik, Karim},
  month = nov,
